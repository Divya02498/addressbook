pipeline {
    agent any

    stages {
        stage('Compile') {
            steps {
                echo 'Compiling Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing Hello World'
            }
        }
        stage('Package') {
            steps {
                echo 'Packing Hello World'
            }
        }
    }
}
